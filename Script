local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

--
----
local CF = CFrame.new
local LatestRoom = game:GetService("ReplicatedStorage").GameData.LatestRoom
local ChaseStart = game:GetService("ReplicatedStorage").GameData.ChaseStart
local BrightnessVariable = false
----
game:GetService("RunService").RenderStepped:Connect(function()
	if BrightnessVariable == true then
		game.Lighting.ClockTime = 12.5
		game.Lighting.Ambient = Color3.new(1,1,1)
		game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
		game.Lighting.Brightness = 1
        game.Lighting.FogEnd = 100000
	end
end)

function ToggledBrighted()
	BrightnessVariable = not BrightnessVariable
end



--

local Window = Rayfield:CreateWindow({
	Name = "TBN's DoorsSploit",
	LoadingTitle = "TBN Suites",
	LoadingSubtitle = "by t_yzi#7379",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
	Discord = {
		Enabled = true,
		Invite = "TN5SdDkhtJ", -- The Discord invite code, do not include discord.gg/
		RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Sirius Hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/sirius)",
		FileName = "SiriusKey",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Hello"
	}})

local GameTab = Window:CreateTab("Game") -- Title, Image

local ToggleBrightnessButton = GameTab:CreateButton({
	Name = "Toggle Brightness",
	Callback = function()
		ToggledBrighted()
	end
})

game:GetService("ProximityPromptService").PromptButtonHoldBegan:Connect(function(prompt)
    if Rayfield.Flags["InstantToggle"].Value == true then
        fireproximityprompt(prompt)
    end
end)

local SkipDoorButton = GameTab:CreateButton({
	Name = "Skip Door",
	Flag = "InstantToggle",
	Callback = function()
		pcall(function()
            local HasKey = false
            local CurrentDoor = workspace.CurrentRooms[tostring(game:GetService("ReplicatedStorage").GameData.LatestRoom.Value)]:WaitForChild("Door")
            for i,v in ipairs(CurrentDoor.Parent:GetDescendants()) do
                if v.Name == "KeyObtain" then
                    HasKey = v
                end
            end
            if HasKey then
                game.Players.LocalPlayer.Character:PivotTo(CF(HasKey.Hitbox.Position))
                wait(0.3)
                fireproximityprompt(HasKey.ModulePrompt,0)
                game.Players.LocalPlayer.Character:PivotTo(CF(CurrentDoor.Door.Position))
                wait(0.3)
                fireproximityprompt(CurrentDoor.Lock.UnlockPrompt,0)
            end
            if LatestRoom == 50 then
                CurrentDoor = workspace.CurrentRooms[tostring(LatestRoom+1)]:WaitForChild("Door")
            end
            game.Players.LocalPlayer.Character:PivotTo(CF(CurrentDoor.Door.Position))
            wait(0.3)
            CurrentDoor.ClientOpen:FireServer()
        end)
	end
})




local SpawnMonsterTab = Window:CreateTab("Monsters") -- Title, Image



local OtherTab = Window:CreateTab("Other") -- Title, Image





local MiscTab = Window:CreateTab("Game") -- Title, Image
local DestroyWindow = MiscTab:CreateButton({
	Name = "Destroy UI",
	Callback = function()
		Rayfield:Destroy()
	end
})
